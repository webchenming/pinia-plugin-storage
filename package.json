{
  "name": "pinia-plugin-storage",
  "version": "0.0.3",
  "description": "Pinia 持久化存储插件",
  "keywords": [
    "pinia",
    "plugin",
    "vue"
  ],
  "repository": {
    "type": "gitee",
    "url": "https://gitee.com/webchenming/pinia-plugin-storage"
  },
  "author": "chenming",
  "license": "MIT",
  "type": "module",
  "main": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/lib/index.cjs"
    },
    "./es/": "./dist/es/index.d.ts",
    "./lib/": "./dist/es/index.mjs",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "dev": "pnpm -C play dev",
    "build": "rollup -cw",
    "release": "bumpp && pnpm publish"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/node": "^18.15.11",
    "@vitest/ui": "^0.30.1",
    "@webchen/eslint-config": "^0.0.5",
    "bumpp": "^9.1.0",
    "eslint": "^8.38.0",
    "pinia": "^2.0.34",
    "pinia-plugin-storage": "workspace:*",
    "rollup": "^3.21.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "vitest": "^0.30.0",
    "vue": "^3.2.47"
  }
}
