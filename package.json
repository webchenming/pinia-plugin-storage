{
  "name": "pinia-plugin-storage",
  "version": "0.1.2",
  "description": "Pinia 持久化存储插件",
  "keywords": [
    "pinia",
    "plugin",
    "vue"
  ],
  "repository": {
    "type": "gitee",
    "url": "https://gitee.com/webchenming/pinia-plugin-storage"
  },
  "author": "chenming",
  "license": "MIT",
  "type": "module",
  "main": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/lib/index.cjs"
    },
    "./es/": "./dist/es/index.d.ts",
    "./lib/": "./dist/es/index.mjs",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "dev": "pnpm -C play dev",
    "build:docs": "pnpm -C play build",
    "build": "rollup -c",
    "build:dev": "rollup -cw",
    "bumpp": "bumpp",
    "release": "pnpm publish --no-git-checks"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.18",
    "@vitest/ui": "^0.30.1",
    "@webchen/eslint-config": "^0.0.13",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "pinia": "^2.1.3",
    "pinia-plugin-storage": "workspace:*",
    "rollup": "^3.25.1",
    "rollup-plugin-node-externals": "^5.1.3",
    "vitest": "^0.30.1",
    "vue": "^3.3.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
